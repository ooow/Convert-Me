    "travis-ci-test": "jest --updateSnapshot"
  },
  "dependencies": {
    "@expo/vector-icons": "^6.3.1",
    "babel-eslint": "^8.2.6",
    "color": "^3.0.0",
    "expo": "^26.1.0",
    "hoist-non-react-statics": "2.5.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "16.3.0-alpha.1",
    "react-native": "^0.54.0",
    "react-native-dropdownalert": "3.4.0",
    "react-native-extended-stylesheet": "^0.8.1",
    "react-navigation": "2.2.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0"
